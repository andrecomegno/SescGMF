<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.example.sescgmf.Home"
        android:label="home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_home_to_training"
            app:destination="@id/training" />
    </fragment>
    <fragment
        android:id="@+id/profile"
        android:name="com.example.sescgmf.Profile"
        android:label="profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/training"
        android:name="com.example.sescgmf.Training"
        android:label="training"
        tools:layout="@layout/fragment_training" >
        <action
            android:id="@+id/action_training_to_home"
            app:destination="@id/home" />
    </fragment>
    <fragment
        android:id="@+id/lesson"
        android:name="com.example.sescgmf.Lesson"
        android:label="fragment_lesson"
        tools:layout="@layout/fragment_lesson" />
</navigation>