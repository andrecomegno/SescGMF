<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.SescGMF" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:statusBarColor">@android:color/transparent</item>>
        <item name="android:windowTranslucentStatus">false</item>
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="Theme.SescGMF" parent="Base.Theme.SescGMF" />

    <!-- Main Menu. -->
    <style name="Widget.App.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
    </style>

    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorOnPrimary">@color/white</item>
    </style>

    <!-- Menu Start Training -->
    <style name="Widget.App.BottomNavigationView_2" parent="Widget.MaterialComponents.BottomNavigationView.Colored">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView_2</item>
    </style>

    <style name="ThemeOverlay.App.BottomNavigationView_2" parent="">
        <item name="colorOnPrimary">@color/white</item>
        <item name="android:layout_marginBottom">10dp</item>
    </style>

    <style name="TextAppearance.MaterialComponents.Caption" parent="">
        <item name="android:textSize">18sp</item>
    </style>


</resources>